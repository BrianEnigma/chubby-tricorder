// Interactively get an entry. Return the entry.
void cyclePictureDataEntry(char *entry, unsigned char maxLength)
{
  unsigned int broadcastCounter = 100;
  unsigned int displayCounter = 0;
  unsigned char displayCycle = 0;
  char key = 0;
  char pos = 0;
  *entry = 0;

  while(1)
  {
    if (0 == displayCounter)
    {
        display.clearDisplay();
        switch(displayCycle)
        {
            case 0:
                display.setTextSize(1);
                display.setTextColor(WHITE, BLACK);
                display.setCursor(0,0);
                display.print(F("Probe "));
                switch(runMode)
                {
                  case 'A': display.print(F("0")); break;
                  case 'B': display.print(F("20")); break;
                  case 'C': display.print(F("40")); break;
                  case 'D': display.print(F("60")); break;
                  case 'E': display.print(F("80")); break;
                }
                display.print(F("% repaired"));
                
                display.setCursor(0, SSD1306_LCDHEIGHT / 8 * 4);
                display.print(F("Enter "));
                display.print('C' == runMode ? F("6") : F("4"));
                display.println(F("-digit memory\n"
                    "address of repair\n"
                    "routine to execute:"));
                break;
            case 1:
                switch(runMode) // I'd love for these to be arrays of Flash pointers to binary arrays, but the Arduino environment makes that difficult...
                {
                  case 'A': display.drawBitmap(PROBE1_1_X, PROBE1_1_Y, PROBE1_1, PROBE1_1_WIDTH, PROBE1_1_HEIGHT, WHITE); break;
                  case 'B': display.drawBitmap(PROBE2_1_X, PROBE2_1_Y, PROBE2_1, PROBE2_1_WIDTH, PROBE2_1_HEIGHT, WHITE); break;
                  case 'C': display.drawBitmap(PROBE3_1_X, PROBE3_1_Y, PROBE3_1, PROBE3_1_WIDTH, PROBE3_1_HEIGHT, WHITE); break;
                  case 'D': display.drawBitmap(PROBE4_1_X, PROBE4_1_Y, PROBE4_1, PROBE4_1_WIDTH, PROBE4_1_HEIGHT, WHITE); break;
                  case 'E': display.drawBitmap(PROBE5_1_X, PROBE5_1_Y, PROBE5_1, PROBE5_1_WIDTH, PROBE5_1_HEIGHT, WHITE); break;
                }
                break;
            case 2:
                switch(runMode)
                {
                  case 'A': display.drawBitmap(PROBE1_2_X, PROBE1_2_Y, PROBE1_2, PROBE1_2_WIDTH, PROBE1_2_HEIGHT, WHITE); break;
                  case 'B': display.drawBitmap(PROBE2_2_X, PROBE2_2_Y, PROBE2_2, PROBE2_2_WIDTH, PROBE2_2_HEIGHT, WHITE); break;
                  case 'C': display.drawBitmap(PROBE3_2_X, PROBE3_2_Y, PROBE3_2, PROBE3_2_WIDTH, PROBE3_2_HEIGHT, WHITE); break;
                  case 'D': display.drawBitmap(PROBE4_2_X, PROBE4_2_Y, PROBE4_2, PROBE4_2_WIDTH, PROBE4_2_HEIGHT, WHITE); break;
                  case 'E': display.drawBitmap(PROBE5_2_X, PROBE5_2_Y, PROBE5_2, PROBE5_2_WIDTH, PROBE5_2_HEIGHT, WHITE); break;
                }
                break;
            case 3:
                switch(runMode)
                {
                  case 'A': display.drawBitmap(PROBE1_3_X, PROBE1_3_Y, PROBE1_3, PROBE1_3_WIDTH, PROBE1_3_HEIGHT, WHITE); break;
                  case 'B': display.drawBitmap(PROBE2_3_X, PROBE2_3_Y, PROBE2_3, PROBE2_3_WIDTH, PROBE2_3_HEIGHT, WHITE); break;
                  case 'C': display.drawBitmap(PROBE3_3_X, PROBE3_3_Y, PROBE3_3, PROBE3_3_WIDTH, PROBE3_3_HEIGHT, WHITE); break;
                  case 'D': display.drawBitmap(PROBE4_3_X, PROBE4_3_Y, PROBE4_3, PROBE4_3_WIDTH, PROBE4_3_HEIGHT, WHITE); break;
                  case 'E': display.drawBitmap(PROBE5_3_X, PROBE5_3_Y, PROBE5_3, PROBE5_3_WIDTH, PROBE5_3_HEIGHT, WHITE); break;
                }
                break;
            case 4:
                switch(runMode)
                {
                  case 'A': display.drawBitmap(PROBE1_4_X, PROBE1_4_Y, PROBE1_4, PROBE1_4_WIDTH, PROBE1_4_HEIGHT, WHITE); break;
                  case 'B': display.drawBitmap(PROBE2_4_X, PROBE2_4_Y, PROBE2_4, PROBE2_4_WIDTH, PROBE2_4_HEIGHT, WHITE); break;
                  case 'C': display.drawBitmap(PROBE3_4_X, PROBE3_4_Y, PROBE3_4, PROBE3_4_WIDTH, PROBE3_4_HEIGHT, WHITE); break;
                  case 'D': display.drawBitmap(PROBE4_4_X, PROBE4_4_Y, PROBE4_4, PROBE4_4_WIDTH, PROBE4_4_HEIGHT, WHITE); break;
                  case 'E': display.drawBitmap(PROBE5_4_X, PROBE5_4_Y, PROBE5_4, PROBE5_4_WIDTH, PROBE5_4_HEIGHT, WHITE); break;
                }
                break;
        }
        display.setCursor(0, SSD1306_LCDHEIGHT / 8 * 7);
        display.print(">");
        display.print(entry);
        display.setTextColor(BLACK, WHITE); // 'inverted' text
        display.print(" ");
        display.setTextColor(WHITE, BLACK);
        display.display();
    }
    key = keypad.getKey();
    if (key >= '0' && key <= '9' && pos < maxLength)
    {
      entry[pos] = key;
      entry[pos + 1] = 0;
      pos++;
    }
    if ('*' == key && pos > 0)
    {
      pos--;
      entry[pos] = 0;
    }
    if (0 == displayCounter || 0 != key)
    {
        display.fillRect(0, SSD1306_LCDHEIGHT / 8 * 7, SSD1306_LCDWIDTH, SSD1306_LCDHEIGHT, BLACK);
        display.setCursor(0, SSD1306_LCDHEIGHT / 8 * 7);
        display.print(">");
        display.print(entry);
        display.setTextColor(BLACK, WHITE); // 'inverted' text
        display.print(" ");
        display.setTextColor(WHITE, BLACK);
        display.display();
    }
    if ('#' == key)
      return;
    delay(10);
    // Broadcast run mode via RF every 30ms of cumulative delay()
    // function calls, which roughly equals 50ms of wall time.
    if (broadcastCounter++ >= 3)
    {
        Serial.write(runMode);
        broadcastCounter = 0;
    }
    if (displayCounter++ > 400)
    {
        displayCounter = 0;
        displayCycle = (displayCycle + 1) % 5; // four pictures plus a text screen
    }
  }
}

unsigned char handleEntry(const char *entry)
{
  if ('A' == runMode && strcmp_P(entry, PSTR("4985")) == 0)
  {
    doRepair();
    return 1;
  }
  if ('B' == runMode && strcmp_P(entry, PSTR("3144")) == 0)
  {
    doRepair();
    return 1;
  }
  if ('C' == runMode && strcmp_P(entry, PSTR("147713")) == 0)
  {
    doRepair();
    return 1;
  }
  if ('D' == runMode && strcmp_P(entry, PSTR("4455")) == 0)
  {
    doRepair();
    return 1;
  }
  if ('E' == runMode && strcmp_P(entry, PSTR("4982")) == 0)
  {
    doRepair();
    return 1;
  }
  
  if (strcmp_P(entry, PSTR("911911")) == 0)
  {
    runMode = 'E';
